{
    "name": "maniaplanet/manialib-manialink",
    "type": "library",
    "description": "Object-oriented PHP library for writing Manialink interfaces.",
    "keywords": ["ManiaPlanet", "ShootMania", "TrackMania", "XML", "Manialink"],
    "license": "MIT",
    "homepage": "https://github.com/maniaplanet/manialib-manialink",
    "authors": [
        {
            "name": "Nadeo",
            "email": "contact@nadeo.com",
            "homepage": "http://www.nadeo.com/"
        }
    ],
    "support": {
        "forum": "http://forum.maniaplanet.com/viewforum.php?f=40"
    },
    "require": {
        "maniaplanet/manialib-xml": "0.2.*@dev"
    },
     "require-dev": {
        "symfony/console": "~2.4",
        "symfony/stopwatch": "~2.4"
    },
    "autoload": {
        "psr-4": {"ManiaLib\\": "src/ManiaLib/"}
    },
    "archive": {
        "exclude": ["/bin", "/examples"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}